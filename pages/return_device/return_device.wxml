<navbar is="components/navbar/index" parameter='{{parameter}}'></navbar>
<form bindsubmit="submit">
  <view class="inp_box">
    <text>订单号：</text>
    <input placeholder="请输入订单号" class="inp" name="order_id" disabled="false" value="{{order_id}}"></input>
  </view>


  <view class="inp_box">
    <text>归还时间：</text>
    <input placeholder="请输入归还时间" class="inp" disabled="false" value="{{today_date}}"></input>
  </view>

  <view class="inp_box">
    <text>快递公司：</text>
    <input placeholder="请输入快递公司" class="inp" name="express_company"></input>
  </view>

  <view class="inp_box">
    <text>快递单号：</text>
    <input placeholder="请输入快递单号" class="inp" name="express_id"></input>
  </view>



  <input disabled="false" name="return_time" value="{{timestamp}}" hidden="true"></input>

  <button form-type="submit" class="btn_submit">提 交</button>



</form>